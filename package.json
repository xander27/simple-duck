{
  "name": "simple-duck",
  "version": "0.0.4",
  "description": "Simple implementation of Duck module concept",
  "main": "lib/index.js",
  "author": "Alexander Borovkov",
  "homepage": "https://github.com/xander27/simple-duck",
  "license": "MIT",
  "scripts": {
    "test": "yarn run compile && jest",
    "compile": "babel -d lib/ src/",
    "prepublish": "yarn run compile && yarn run compile:flow-types",
    "compile:flow-types": "flow gen-flow-files src/index.js > lib/index.js.flow && npm run fix-flow-issue-2830",
    "fix-flow-issue-2830": "replace-in-file \"/default function(\\(.*\\)):/\" \"default \\$1 =>\" lib/index.js.flow --isRegex"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "flow-bin": "^0.71.0",
    "jest": "^22.1.2",
    "regenerator-runtime": "^0.11.1",
    "replace-in-file": "^3.4.0"
  },
  "keywords": [
    "redux",
    "react",
    "duck-modules",
    "duck",
    "module",
    "classes"
  ],
  "dependencies": {
    "lodash.camelcase": "^4.3.0",
    "lodash.get": "^4.4.2"
  }
}
